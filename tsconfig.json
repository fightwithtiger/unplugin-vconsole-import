{
  "compilerOptions": {
    "target": "es2017",
    "module": "esnext",
    "lib": ["esnext"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "jsx": "preserve",
    "skipLibCheck": true,
    "strictNullChecks": true,
    "resolveJsonModule": true,
    "types": ["vite/client"]
  },
  "exclude": ["dist/**", "node_modules/**"]
}
